{
  "name": "classroom",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "types": "src/types/*.d.ts",
  "scripts": {
    "tsc": "tsc --project tsconfig-electron.json",
    "build": "snowpack build",
    "dev": "snowpack dev",
    "start": "npm run dev",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "electron": "wait-on http://localhost:8080 && electron dist/main.js",
    "electron-dev": "npm run tsc && concurrently \"npm run dev\" \"npm run electron\"",
    "preelectron-pack": "npm run tsc && npm run build",
    "electron-pack": "electron-builder -l"
  },
  "devDependencies": {
    "@iconify/svelte": "^2.2.0",
    "@snowpack/app-scripts-svelte": "^2.0.1",
    "@snowpack/plugin-webpack": "^3.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "electron": "^18.0.1",
    "electron-builder": "^22.14.13",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.12",
    "sass": "^1.49.9",
    "snowpack": "^3.8.8",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.0.0",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/snowpack-env": "^2.3.4",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-serve": "^1.1.0",
    "mime-types": "^2.1.35",
    "sirv-cli": "^2.0.0",
    "svelte": "^3.0.0",
    "svelte-spa-router": "^3.2.0"
  },
  "build": {
    "icon": "public/favicon.png",
    "productName": "ClassRoom",
    "files": [
      "public/build/**/*",
      "dist/main.js",
      "dist/preload.js",
      "dist/ipc/*"
    ],
    "win": {},
    "linux": {},
    "mac": {
      "target": [
        "dmg"
      ]
    }
  }
}
